
    	$('#calendar').fullCalendar({
			header: {
				left: 'prev,next today',
				center: 'title',
				right: 'month'
			},
			//,agendaWeek,agendaDay
			defaultDate: moment().format("YYYY-MM-DD"),
			editable: false,
			eventLimit: true, // allow "more" link when too many events
			events: {
		        url: '/eventbyfundanduserandduration',
		        type: 'GET',
		        data: {
		        	fund: fund==null?null:fund[0],
		        	user: user==null?null:user[0]
		        },
		        error: function() {
		            alert('there was an error while fetching events!');
		        }
		        //color: '#81ccc4',   // a non-ajax option
		        //textColor: 'white' // a non-ajax option
		    },
		    
		    events: function(start, end, timezone, callback) {
		        $.ajax({
		            url: '/eventbyfundanduserandduration',
		            method: 'GET',
		            dataType: 'json',
		            data: {
			        	fund: fund==null?null:fund[0],
			        	user: user==null?null:user[0],
			        	start: start,
			        	end: end
			        },
		            success: function(data) {
		                //update cp filter with data.corpActionNames
		                //update fundtype filer with data.fundTypes
		                callback(data.events); // Pass the events to the calendar
		            }
		        });
		    },
		    
		    
		    
		    
