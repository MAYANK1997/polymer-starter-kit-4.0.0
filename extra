 let actionNames = [];
                let tickers = [];

                corporateActions.forEach(action => {
                    actionNames.push(action.corporateActionName);
                    tickers.push(action.tickers.join("\n")); // Join tickers with line breaks
                });

                // Create a row with the date, actions (line break-separated), and tickers (line break-separated)
                const row = `<tr>
                                <td>${date}</td>
                                <td class="multiline">${actionNames.join("\n")}</td>
                                <td class="multiline">${tickers.join("\n")}</td>
                             </tr>`;
                tableBody.innerHTML += row;
            });
