 function displayTableData() {
            const tableBody = document.getElementById('table-body');

            // Clear any existing rows
            tableBody.innerHTML = '';

            // Iterate over the API data
            apiData.forEach(eventGroup => {
                const date = eventGroup.effectiveDate;
                const corporateActions = eventGroup.data;

                // For each corporate action, add rows
                corporateActions.forEach((action, actionIndex) => {
                    const tickers = action.tickers;

                    // Add the first row for the date with the first action and ticker
                    const firstRow = `<tr>
                                        ${actionIndex === 0 ? `<td rowspan="${corporateActions.length}">${date}</td>` : ''}
                                        <td rowspan="${tickers.length}">${action.corporateActionName}</td>
                                        <td>${tickers[0]}</td>
                                    </tr>`;
                    tableBody.innerHTML += firstRow;

                    // Add remaining rows for the additional tickers (if any)
                    for (let i = 1; i < tickers.length; i++) {
                        const row = `<tr>
                                        <td>${tickers[i]}</td>
                                    </tr>`;
                        tableBody.innerHTML += row;
                    }
